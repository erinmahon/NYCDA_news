<%= form_tag "/subscriber-login", method: "post" do %>   
<%=label_tag :email %>   
<%=text_field_tag :email %> 
<%=label_tag :password %>   
<%=password_field_tag :password %>  
<%=submit_tag "Login" %> 
<% end %>