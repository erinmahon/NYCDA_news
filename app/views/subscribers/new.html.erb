<h1>Subscribers#new</h1>
<p>Find me in app/views/subscribers/new.html.erb</p>

<div id="sign_up_form" class="form">
	<%= form_for @subscriber do |f| %>   
		<%=f.label :First_Name %>   
		<%=f.text_field :fname %> 
		<br>
		<%=f.label :Last_name %>   
		<%=f.text_field :lname %>  
		<br>
		<%=f.label :email %>   
		<%=f.email_field :email %>  
		<br>
		<%=f.label :password %>   
		<%=f.password_field :password %>  
		<br>
		<%=f.label :city %>   
		<%=f.text_field :city %> 
		<br>
		<%=f.label :state %>   
		<%=f.select :state, options_for_select(us_states) %>  
		<br>  
		<div class="button"><%=f.submit :create %></div>
	<% end %>
</div>
